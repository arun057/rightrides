<div id="RRContainer" class="panel">
	<div id="header"></div>
	<div id="RRTools" class="panel">
		<div id="RRChatPane" class="pane">
		</div>
		<div id="RRMapPane" class="pane">
			<div class="map_div">
				<form action="#" onsubmit="geocode(this.address.value); return false" id="RRMapForm">
					<div id="mapaddressInput">
						<input type="text" style="width: 330px;" name="address" value="26 Court St, New York to 52 Chambers St, New York" />
						<input type="submit" value="Go!" class="btn"/>
						<input type="button" value="Clear" class="btn" onclick="clearCustomMarkers();"/>
						<input type="button" value="Refresh Drivers" class="btn" onclick="refreshMarkers();"/>
					</div>
					<div id="map_canvas" style="width: 100%; height: 580px;"></div>
				</form>
			</div>
		</div>
	</div>
	<div id="RRSpreadsheet" class="panel">
		<div id="RRSpreadsheetPane" class="pane">
			<iframe src='https://docs.google.com/a/rightrides.org/spreadsheet/ccc?key=0Av9gn1XNoLa6dFJKYU53cmZzNk5WRTZ1SXJ5WG1oYnc#gid=1'>
			</iframe>
		</div>
	</div>
	<footer class="footer"><img src="/assets/logo.jpg" style="display:none;"/>
</div>

<script type="text/javascript" src="http://www.google.com/jsapi"></script>
<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=false"></script>